{
    "name": "emoji",
    "dimensions": {
        "gate_width": 40,
        "gate_height": 40,
        "gate_spacing": 20,
        "line_spacing": 60,
        "padding": 40,
        "label_offset": 50,
        "barrier_padding": 20,
        "reverse_qubit_order": false,
        "reverse_classical_order": false
    },
    "styles": {
        "background": "#fffdf7",
        "text": "#333333",
        "label_font": {
            "family": "calibri",
            "size": 17
        },
        "qubit_wire": {
            "style": "straight",
            "stroke": "#cccccc",
            "stroke_width": 3,
            "dasharray": ""
        },
        "connection_line": {
            "style": "wave",
            "stroke": "#aaaaaa",
            "stroke_width": 2,
            "amplitude": 3,
            "wavelength": 12
        },
        "measurement_line": {
            "style": "straight",
            "stroke": "#bbbbbb",
            "stroke_width": 2,
            "dasharray": "2,2"
        },
        "barrier": {
