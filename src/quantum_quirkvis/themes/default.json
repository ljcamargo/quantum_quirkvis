{
    "name": "default",
    "dimensions": {
        "gate_width": 40,
        "gate_height": 40,
        "gate_spacing": 20,
        "line_spacing": 60,
        "padding": 40,
        "label_offset": 50,
        "barrier_padding": 20,
        "reverse_qubit_order": false,
        "reverse_classical_order": false
    },
    "styles": {
        "background": "#fafafa",
        "text": "#555555",
        "label_font": {
            "family": "calibri",
            "size": 16
        },
        "qubit_wire": {
            "style": "straight",
            "stroke": "#777777",
            "stroke_width": 3,
            "dasharray": ""
        },
        "connection_line": {
            "style": "wave",
            "stroke": "#444444",
            "stroke_width": 2,
            "amplitude": 3,
            "wavelength": 12
        },
        "barrier": {
            "style": "straight",
            "stroke": "gray",
            "stroke_width": 2,
            "dasharray": "4,4"
        }
    },
    "shapes": {
        "gate": {
            "type": "circle",
            "radius": 20,
            "fill": "#d4b6e8",
            "stroke": "#777777",
            "stroke_width": 2
        },
        "control_dot": {
            "type": "circle",
            "radius": 5,
            "fill": "#444444",
            "stroke": "none"
        },
        "target_plus": {
            "type": "plus_circle",
            "radius": 12,
            "fill": "#fafafa",
            "stroke": "#444444",
            "stroke_width": 2
        },
        "measurement": {
            "type": "diamond",
            "radius": 18,
            "fill": "#A0A0A0",
            "stroke": "#444444",
            "stroke_width": 1
        },
        "swap_x": {
            "type": "cross",
            "size": 8,
            "stroke": "#222222",
            "stroke_width": 2
        }
    },
    "gates": {
        "h": {
            "fill": "#f0a6a6",
            "label": "H"
        },
        "x": {
            "fill": "#d4b6e8",
            "label": "X"
        },
        "y": {
            "fill": "#d4b6e8",
            "label": "Y"
        },
        "z": {
            "fill": "#d4b6e8",
            "label": "Z"
        },
        "rx": {
            "parametric_mode": "arc",
            "arc_stroke": "#ff5555",
            "arc_stroke_width": 3,
            "arc_radius": 18,
            "label": "Rx",
            "fill": "#ffe0e0"
        },
        "ry": {
            "parametric_mode": "arc",
            "arc_stroke": "#55ff55",
            "arc_stroke_width": 3,
            "arc_radius": 18,
            "label": "Ry",
            "fill": "#e0ffe0"
        },
        "rz": {
            "parametric_mode": "arc",
            "arc_stroke": "#5555ff",
            "arc_stroke_width": 3,
            "arc_radius": 18,
            "label": "Rz",
            "fill": "#e0e0ff"
        },
        "p": {
            "parametric_mode": "arc",
            "arc_stroke": "#f1fa8c",
            "arc_stroke_width": 3,
            "arc_radius": 18,
            "label": "P",
            "fill": "#fdfdfb"
        },
        "measurement": {
            "type": "diamond",
            "label": "M"
        }
    },
    "substitutions": {}
}